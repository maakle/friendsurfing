pod 'GoogleMaps'
pod 'GooglePlaces'


  target 'FriendsurfingTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FriendsurfingUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
